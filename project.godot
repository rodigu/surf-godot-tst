; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "FollowPoint",
"language": "GDScript",
"path": "res://src/components/follow_point.gd"
}, {
"base": "Node",
"class": "State",
"language": "GDScript",
"path": "res://src/components/state.gd"
}, {
"base": "Node",
"class": "StateMachine",
"language": "GDScript",
"path": "res://src/components/state_machine.gd"
}, {
"base": "Node",
"class": "TouchDrag",
"language": "GDScript",
"path": "res://src/components/touch_drag.gd"
} ]
_global_script_class_icons={
"FollowPoint": "",
"State": "",
"StateMachine": "",
"TouchDrag": ""
}

[application]

config/name="Surf-godot-tst"
run/main_scene="res://src/scenes/Game.tscn"
config/icon="res://icon.png"

[display]

window/size/width=360
window/size/height=640
window/size/resizable=false
window/handheld/orientation="portrait"
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[importer_defaults]

texture={
"flags/filter": false
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
